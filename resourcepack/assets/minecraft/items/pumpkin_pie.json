{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/pumpkin_pie"
    },
    "cases": [
      {
        "when": "servercore:apple_pie",
        "model": {
          "type": "model",
          "model": "servercore:item/apple_pie"
        }
      },
      {
        "when": "servercore:berry_pie",
        "model": {
          "type": "model",
          "model": "servercore:item/berry_pie"
        }
      }
    ]
  }
}
